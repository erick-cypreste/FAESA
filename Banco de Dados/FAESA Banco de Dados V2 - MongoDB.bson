/* Este arquivo contem o Script para cria� e carga do Modelo de Dados de Clientes que ser�tilizado durante as aulas de laborat� da linguagem BSON. Nele est�contidos os comandos para cria� das tabelas que comp�em o modelo assim como dos comandos que far�a povoa� dos dados dessas tabelas. 
Profa. Eliana Caus Sampaio */

-- -----------------------------------
-- cria modelo de dados de cliente 
-- -----------------------------------

db.ite.drop()
db.pro.drop()
db.ped.drop()
db.cli.drop()
db.uf.drop()
db.ue.drop()

db.createCollection("ue")
db.ue.insertOne(
	{
		ue_cod: <string>,
		ue_desc: <string>
	}
)

db.createCollection("uf")

db.createCollection("pro")

db.createCollection("cli")

db.createCollection("ped")

db.createCollection("ite")

db.ue.createIndex({ue_cod:1},{unique:true})

db.uf.createIndex({uf_cod:1},{unique:true})

db.pro.createIndex({pro_cod:1},{unique:true})
 
db.cli.createIndex({cli_cod:1},{unique:true})

db.ped.createIndex({ped_cod:1},{unique:true})

db.ite.createIndex({ite_cod:1, pro_cod:1, ite_seq:1},{unique:true})

db.cli.aggregate([
{
    $lookup:
    {
        from: "uf",
        localField: "uf_cod",
        foreignField: "uf_cod",
        as: "uf_desc"
    }
}])

db.ped.aggregate([
{
    $lookup:
    {
        from: "cli",
        localField: "cli_cod",
        foreignField: "cli_cod",
        as: "cli_nome"
    }
}])

db.ite.aggregate([
{
    $lookup:
    {
        from: "pro",
        localField: "pro_cod",
        foreignField: "pro_cod",
        as: "pro_nome"
    }
}])

db.ite.aggregate([
{
    $lookup:
    {
        from: "ped",
        localField: "ped_cod",
        foreignField: "ped_cod",
        as: "ped_vl_liq"
    }
}])

db.pro.aggregate([
{
    $lookup:
    {
        from: "ue",
        localField: "ue_cod",
        foreignField: "ue_cod",
        as: "ue_desc"
    }
}])

